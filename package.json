{
  "name": "es6-starter-babel-eslint-tape",
  "version": "0.0.1",
  "description": "es6 starter project with babel, eslint &amp; tape",
  "main": "index.js",
  "scripts": {
    "test": "nyc tape test/*.js",
    "testw": "nodemon -q -x 'npm test'",
    "build-es5": "babel -d ./lib/es5 --presets babel-preset-es2015 ./src",
    "build-es5-w": "babel -w -d ./lib/es5 --presets babel-preset-es2015 ./src",
    "build": "babel -d ./lib --presets babel-preset-es2015-node5 ./src",
    "build-w": "babel -w -d ./lib --presets babel-preset-es2015-node5 ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spacious/es6-starter-babel-eslint-tape.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spacious/es6-starter-babel-eslint-tape/issues"
  },
  "homepage": "https://github.com/spacious/es6-starter-babel-eslint-tape",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-es2015-node5": "^1.1.1",
    "eslint": "^1.10.3",
    "nodemon": "^1.8.1",
    "nyc": "^3.2.2",
    "tape": "^4.3.0"
  }
}
